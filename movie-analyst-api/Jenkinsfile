node {
    stage('Checkout scm') {
        checkout scm
    }

    stage('Run test') {
        def docker = docker.image('node:10.11.0-alpine').withRun() { c ->
            sh 'apt-get install -y npm'
            echo 'Performing tests'
            sh 'npm install --only=dev'
            sh 'npm test'
        }
    }

    stage('Build docker image') {
        echo "Not yet"
    }
}