node {
    stage('Checkout scm') {
        checkout scm
    }

    stage('Run test') {
        docker.image('node:10.11.0-alpine').inside() {
            echo 'Performing tests'
            sh 'npm install'
            sh 'npm test'
        }
    }

    stage('Build docker image') {
        echo "Not yet"
    }
}